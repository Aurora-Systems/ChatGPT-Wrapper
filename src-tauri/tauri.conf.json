{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "https://chat.openai.com/",
    "distDir": "https://chat.openai.com/",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "ChatGPT",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.aurora.systems",
      "icon": [
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Chat GPT",
        "width": 800,
        "height": 600
      }
    ]
  }
}
